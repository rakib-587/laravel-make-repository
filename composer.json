{
    "name": "rakib-587/laravel-make-repository",
    "description": "Laravel Artisan command to generate repositories with optional model binding.",
    "type": "library",
    "keywords": [
        "rakib-587",
        "laravel",
        "repository",
        "laravel-repository",
        "laravel-make-repository"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Md Rakibul Islam",
            "email": "rakib.dev.000@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "illuminate/console": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Rakib\\MakeRepository\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rakib\\MakeRepository\\MakeRepositoryServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
